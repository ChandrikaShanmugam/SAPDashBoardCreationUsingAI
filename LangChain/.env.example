# SAP Dashboard Configuration
# Copy this file to .env and fill in your values

# =======================
# DATA SOURCE CONFIGURATION
# =======================
# Options: 'csv' or 'api'
DATA_SOURCE=csv

# =======================
# SAP CONNECTION TYPE
# =======================
# Options: 'odata', 'rfc', or 'hana'
SAP_CONNECTION_TYPE=odata

# =======================
# SAP OData Configuration
# =======================
# For SAP Gateway / S/4HANA OData services
SAP_BASE_URL=https://your-sap-system.com:8000
SAP_USERNAME=your_username
SAP_PASSWORD=your_password
SAP_CLIENT=100

# =======================
# SAP RFC Configuration
# =======================
# For direct RFC/BAPI calls (requires pyrfc library)
SAP_SERVER=sap-server.company.com
SAP_SYSNR=00

# =======================
# SAP HANA Configuration
# =======================
# For direct HANA database connection (requires hdbcli library)
SAP_HANA_ADDRESS=hana-server.company.com
SAP_HANA_PORT=30015

# =======================
# LLM Configuration
# =======================
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_if_needed
